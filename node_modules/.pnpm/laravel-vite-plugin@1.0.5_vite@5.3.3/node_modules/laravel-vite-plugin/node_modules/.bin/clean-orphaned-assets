#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/gregory/gregData/phpstormProjects/Laravel11_todoApp/node_modules/.pnpm/laravel-vite-plugin@1.0.5_vite@5.3.3/node_modules/laravel-vite-plugin/bin/node_modules:/home/gregory/gregData/phpstormProjects/Laravel11_todoApp/node_modules/.pnpm/laravel-vite-plugin@1.0.5_vite@5.3.3/node_modules/laravel-vite-plugin/node_modules:/home/gregory/gregData/phpstormProjects/Laravel11_todoApp/node_modules/.pnpm/laravel-vite-plugin@1.0.5_vite@5.3.3/node_modules:/home/gregory/gregData/phpstormProjects/Laravel11_todoApp/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/gregory/gregData/phpstormProjects/Laravel11_todoApp/node_modules/.pnpm/laravel-vite-plugin@1.0.5_vite@5.3.3/node_modules/laravel-vite-plugin/bin/node_modules:/home/gregory/gregData/phpstormProjects/Laravel11_todoApp/node_modules/.pnpm/laravel-vite-plugin@1.0.5_vite@5.3.3/node_modules/laravel-vite-plugin/node_modules:/home/gregory/gregData/phpstormProjects/Laravel11_todoApp/node_modules/.pnpm/laravel-vite-plugin@1.0.5_vite@5.3.3/node_modules:/home/gregory/gregData/phpstormProjects/Laravel11_todoApp/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/clean.js" "$@"
else
  exec node  "$basedir/../../bin/clean.js" "$@"
fi
